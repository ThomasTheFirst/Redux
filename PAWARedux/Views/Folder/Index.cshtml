@model IEnumerable<PAWARedux.Models.Folder>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User.Username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InFolderID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateDateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FolderName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.User.Username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InFolderID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateDateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FolderName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FolderID }) |
            @Html.ActionLink("Details", "Details", new { id=item.FolderID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FolderID })
        </td>
    </tr>
}

</table>
